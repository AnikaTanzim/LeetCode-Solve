        "class Solution:\n",
        "    def canMakeArithmeticProgression(self, arr: List[int]) -> bool:\n",
        "        arr.sort()\n",
        "        diff = arr[1] - arr[0]\n",
        "\n",
        "        for i in range(1,len(arr)-1):\n",
        "            if arr[i+1]- arr[i] != diff:\n",
        "                return False\n",
        "        return True"
      ],
