        "class Solution:\n",
        "    def isAnagram(self, s: str, t: str) -> bool:\n",
        "        s= list(s)\n",
        "        t= list(t)\n",
        "        s.sort()\n",
        "        t.sort()\n",
        "        if s==t:\n",
        "            return True\n",
        "        else:\n",
        "            return False"
