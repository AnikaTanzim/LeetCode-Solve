        "class Solution:\n",
        "    def missingNumber(self, nums: List[int]) -> int:\n",
        "        ans= len(nums)\n",
        "        for i in range (0, max(nums)+1):\n",
        "            if i not in nums:\n",
        "                ans =i\n",
        "\n",
        "        return ans\n",
        ""
