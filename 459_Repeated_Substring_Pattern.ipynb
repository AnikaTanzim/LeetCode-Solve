        "class Solution:\n",
        "    def repeatedSubstringPattern(self, s: str) -> bool:\n",
        "        ss = (s + s)[1:-1]\n",
        "        if ss.find(s) != -1:\n",
        "            return True\n",
        "        else:\n",
        "            return False\n"
